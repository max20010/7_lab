#include "stdio.h"
int main()
{
	int i,j=0,c=0;
	char x[100],x1[100];
	gets(x);
	for(i=0;i<strlen(x);i++){
		if (x[i]!= ' '){
			x[i]=tolower(x[i]);
			x1[j]=x[i];
			j++;
		}
	}

	for(i=0;i<strlen(x1)/2;i++){
		if(x1[i]!=x1[strlen(x1)-i-1]){
			c++;
			break;
		}
	}
	
	if (c==0){
		printf("yes");	
	}else{
		printf("no");
	}

	return 0;
}
